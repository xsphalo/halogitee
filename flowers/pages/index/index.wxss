.swiper {
 height: 400rpx;
 width: 100%;
 text-align: center;
}
.swiper .slide-image{
 height: 100%;
 width: 100%;
}
.prodlist{
  /* height: 320rpx; */
  padding: 16rpx 49rpx;
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  background: #fff;
  margin-bottom: 20rpx;
}
.prodname{
  width: 163rpx;
  height: 150rpx;
  text-align: center;
  margin-bottom: 28rpx;
}

.prodname image{
  width: 90rpx;
  height: 90rpx;
}
.prodname text{
  display: block;
  font-size: 12px;
  color: #333;
}
.gonggao{
  background: #fff;
  padding:0 20rpx;
  height: 210rpx;
  margin-bottom: 20rpx;
 
}
.gonggao image{
  width: 66rpx;
  height: 44rpx;
  
}
.activiare{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.activiare view{
  text-align: center;
  

}
.activiare view view{
  font-size: 12px;
  border: 1px solid #3bcaff;
  height: 40rpx;
  width: 160rpx;
  border-radius: 20px 20px;
  line-height: 40rpx;
}
.activiare view:first-child view{
  color: #3bcaff;
  border-color: #3bcaff;
}
.activiare view:nth-child(2) view{
  color: #ff586f;
  border-color: #ff586f;
}
.activiare view:last-child view{
  color: #ff9b3a;
  border-color: #ff9b3a;
}
.gonggao view.zhaun{
  font-size: 16px;
  color: #333;
  text-align: center;
  height: 90rpx;
  line-height: 90rpx;
}
.prolist{
  background: #fff;
  margin-bottom: 20rpx;

}
.products:after{
  display: table;
  content: '';
  clear: both;
}
.producmsg:nth-child(2n+1){
  margin-right: 20rpx;
}
.producmsg{
  margin-bottom: 20rpx;
}
.prolist .headfont{
  font-size: 16px;
  color: #333;
  text-align: center;
  height: 80rpx;
  border-bottom: 1px solid #e6e6e6;
 
}
.products{
  padding: 20rpx;

}
.producmsg{
  width: 344rpx;
  float: left;
}
.products image{
  width: 344rpx;
  height: 344rpx;
  margin-bottom: 10rpx;
}
.prodeat{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.prodeat .procart image{
  width: 56rpx;
  height: 56rpx;
}

.prodea-msg view:first-child{
  font-size: 14px;
  color: #333;
}
.prodea-msg view:last-child{
  font-size: 16px;
  color: #ff5858;
}
.headfont{
  padding-top: 24rpx;
  padding-bottom: 10rpx;
}
.headfont text{
  display: block;
  margin-bottom: -10rpx;
}
.headfont image{
  width: 102rpx;
  height: 16rpx;
}

